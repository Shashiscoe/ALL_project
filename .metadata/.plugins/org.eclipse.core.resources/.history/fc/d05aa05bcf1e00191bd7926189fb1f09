package package1;
/*
 * for n=3
the program should print
1 1 1 2
3 2 2 2
3 3 3 4
 */
public class pattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int count=1;
		for(int i=0;i<3;i++)
		{
			if(i%2==0)
			{
				for(int j=0;j<2;j++)
				{
					System.out.print(count+" ");
				}
				System.out.print(count+1);
				System.out.println();
				count++;
			}
			else
			{
				System.out.print((count+1)+" ");
				for(int j=0;j<2;j++)
				{
					System.out.print(count);
				}
				
				count++;
			}
			
		}

	}

}
