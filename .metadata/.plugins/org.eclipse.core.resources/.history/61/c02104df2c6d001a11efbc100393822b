package Singleton;

public class SingletonP {
	private static SingletonP obj=null;
	private SingletonP()
	{
		if(obj==null)
		{
			obj=new SingletonP();
		}
	}
	public static SingletonP getInstance()
	{
		return obj;
	}
	public static void main(String[] args) {
		
		SingletonP obj1=new SingletonP();
		SingletonP obj2=new SingletonP();
		SingletonP obj3=new SingletonP();
			}

}
