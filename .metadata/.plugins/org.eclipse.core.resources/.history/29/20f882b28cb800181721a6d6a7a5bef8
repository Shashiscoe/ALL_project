package package1;

public class IpValidation {

	public static void main(String[] args) {
		String ip="128.2.650.55";
		String arr[]=ip.split("\\.");
		String status="Valid";
		for(String x:arr)
		{
			System.out.println(x);
			int y=Integer.parseInt(x);
			if(y>255 && y<0)
			{
				status="Invalid";
			}
		}
		System.out.println(status);
	}

}
