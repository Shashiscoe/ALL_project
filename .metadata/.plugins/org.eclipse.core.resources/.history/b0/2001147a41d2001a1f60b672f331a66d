package VerifyConcept;

public class StringMutabilityCheck {

	public static void main(String[] args) {
	
		String s1=new String("shashi");
		String s2=new String("shashi");
		System.out.println(s1.hashCode());
		System.out.println(s1);
		s1.concat(" kumar");
		s1=s1+" kumar";
		System.out.println(s1.hashCode());
		System.out.println(s1.hashCode());
		
		System.out.println(s1);

	}

}
