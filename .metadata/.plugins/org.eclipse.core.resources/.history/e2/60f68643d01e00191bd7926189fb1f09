package package1;

import java.util.Scanner;

public class pattern3 {
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		System.out.println("enter the number:");
		int num=scan.nextInt();
		for(int i=0;i<num;i++)
		{
			for(int j=num;j>i;j++)
			{
				System.out.print(" ");
			}
		}
	}

}
