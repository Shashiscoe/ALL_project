		int len=input1.length();
		int x=' '; 
		for(int i=0;i<len;i++)
		{
			if(input2==0 )
			{
				if(input1.charAt(i)=='A' ||input1.charAt(i)=='E' ||input1.charAt(i)=='I' ||
				  input1.charAt(i)=='O' ||input1.charAt(i)=='U' )
				{
					;
				}
				else
				{
					if(input1.charAt(i)==' ')
					{
						;
					}
					else
					{
						sum=sum+((input1.charAt(i)-64));
					}
				}
			}
			else
			{
				if(input1.charAt(i)==' ')
				{
					;
				}
