package TreeMap;
/*
 * TreeMap(Comparator comp): This is an argument constructor and it takes Comparator object to constructs an
  empty tree-based map. It will be sorted by using the Comparator comp.
  
TreeMap(Map map): It creates a TreeMap with the entries from a map, which will be sorted by using the natural 
order of the keys.

TreeMap(SortedMap sortedMap): It also initializes a TreeMap with the entries from sortedMap, which will be sorted 
in the same order as sortedMap.
*/
import java.util.HashMap;
import java.util.Hashtable;
import java.util.Map;
import java.util.TreeMap;

public class TreeMapBasic2 {

	public static void main(String[] args) {
		Map<Integer, String> t1 = new HashMap();
		t1.put(1009, "Shashi");
		t1.put(101, "Ravi");
		t1.put(1085, "kumar");
		t1.put(1079, "Ravi");
		t1.put(103, "kumar");
		System.out.println(t1);
		/*
		TreeMap(SortedMap sortedMap): It also initializes a TreeMap with the entries from sortedMap, which will be sorted 
		in the same order as sortedMap.
		*/
		TreeMap tmap=new TreeMap<>(t1);
		System.out.println(tmap);
		
		Hashtable<Integer, String> hashTable=new Hashtable<>(-1,0.75f);
		
		

	}
}
