import java.util.Arrays;
import java.util.List;
import java.util.function.Consumer;


/*Consumer is a functional interface which has only one method*/

class ConsumerImpl implements Consumer<Integer>
{

	@Override
	public void accept(Integer i) {
		System.out.println(i);
		
	}
	
}
public class DemoConsumer {

	public static void main(String[] args) {
		
		List<Integer> list=Arrays.asList(1,2,3,4,5,6,7);
/*		
		ConsumerImpl c=new ConsumerImpl();
		list.forEach(c);
		
		//We can simplify like this
		System.out.println();
		ConsumerImpl c1=new ConsumerImpl()
				{
			public void accept(Integer i) {
				System.out.println(i);
				
			}
				};
		
		//we can also simplify like that which is nothing but lambda expression 
		list.forEach(c1);*/
		Consumer c3=(i)-> System.out.println(i);
			list.forEach(c3);
			
	}
	
}
