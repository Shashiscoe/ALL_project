package MileStone1;

public class Array9 {

	public static void main(String[] args) {
		int arr[]={10,3,6,1,2,7,9,11,25,8};
		int start,end;
		for(int i=0;i<arr.length;i++)
		{
			int x=arr[i];
			int y=x+1;
			for(int j=i;j<arr.length;j++)
			{
				if(arr[j]==y)
				{
					start=i;
					end=j;
					for(int k=start;k<=end;k++)
					{
						arr[k]=0;
						
					}
				}
				
			}
		}
		int sum=0;
		for(int i=0;i<arr.length;i++)
		{
			sum+=arr[i];
		}
		System.out.println("Sum is:"+sum);
				
		

	}

}
